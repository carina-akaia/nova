{
	"name": "@akaia/commlink-extension-framework",
	"version": "0.1.0",
	"description": "AKAIA CommLink extension framework",
	"license": "MIT",
	"author": "carina.akaia.io",
	"contributors": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/akaia-io/commlink.git"
	},
	"homepage": "https://github.com/akaia-io/commlink#readme",
	"bugs": {
		"url": "https://github.com/akaia-io/commlink/issues"
	},
	"files": ["dist"],
	"private": false,
	"sideEffects": false,
	"type": "module",
	"main": "./../../services/platform/public/packages/extension-framework/index.js",
	"module": "./../../services/platform/public/packages/extension-framework/index.js",
	"types": "./../../services/platform/public/packages/extension-framework/index.d.ts",
	"browser": {},
	"exports": {
		"import": {
			"types": "./../../services/platform/public/packages/extension-framework/index.d.ts",
			"default": "./../../services/platform/public/packages/extension-framework/index.js"
		}
	},
	"typesVersions": {},
	"tsup": {
		"entry": ["src/index.ts"],
		"splitting": false,
		"sourcemap": true,
		"clean": true,
		"treeshake": true
	},
	"scripts": {
		"update-deps": "pnpm up -Li",
		"add:solidui": "pnpm dlx solidui-cli@latest add",
		"lint": "biome lint",
		"format": "biome format --write",
		"typecheck": "tsc --noEmit",
		"watch": "tsup --watch",
		"build": "pnpm format && tsup",
		"test": "concurrently pnpm:test:*",
		"test:client": "vitest",
		"prepublishOnly": "pnpm build"
	},
	"dependencies": {
		"@surrealdb/wasm": "1.0.0-beta.16",
		"@unocss/preset-attributify": "^0.62.2",
		"chart.js": "^4.4.2",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"cmdk-solid": "^1.0.1",
		"corvu": "^0.5.1",
		"embla-carousel-solid": "^8.0.2",
		"jsdom": "^24.0.0",
		"remeda": "^2.11.0",
		"rxdb": "^15.34.1",
		"surrealdb": "^1.0.0",
		"svelte-browser-import": "^0.0.4",
		"tailwind-merge": "^2.3.0",
		"tailwindcss": "^3.4.3",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.3",
		"@types/node": "^20.12.12",
		"@typescript-eslint/eslint-plugin": "^7.9.0",
		"@typescript-eslint/parser": "^7.9.0",
		"concurrently": "^8.2.2",
		"esbuild": "^0.21.3",
		"eslint": "^8.56.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"prettier": "3.0.0",
		"tsup": "^8.0.2",
		"typescript": "^5.4.5",
		"vitest": "^1.6.0"
	},
	"packageManager": "pnpm@9.1.1",
	"engines": {
		"node": ">=22",
		"pnpm": ">=9.0.0"
	}
}
